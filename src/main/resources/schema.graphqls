type Session {
    session_id: ID!
    session_name: String!
    session_length: Int!
    session_description: String!
    speakers: [String]
}

type Query {
    findAllApplications: [Session]!
    countApplications: Int!
}

type Mutation {
    newSession(session_name: String!, session_length: Int!, session_description: String!) : Session!
    deleteSession(id:ID!) : Boolean
    updateSessionName(newSession_name: String!, id:ID!) : Session!
}